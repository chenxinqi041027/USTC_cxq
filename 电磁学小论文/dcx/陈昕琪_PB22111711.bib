@article{_matlab_2022,
	title = {Matlab在大学物理课程电磁学教学中的应用},
	volume = {13},
	issn = {1674-9499},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=TzO8JwpG6uiDNZQtkxohiMnM9GDtfKAZLpFkWX9Jcl3pARyDqihqOeT6PzMDvrj9uFyxjo0DX_CPOwXzGN7dU1f3Xz-CX-e0Hw5ajCBnclTMEw9CvL5C5AI_wvtPDNFhulNAbvmB6e69PD9vDxdzpA==&uniplatform=NZKPT&language=CHS},
	abstract = {由于电磁场概念和空间分布的抽象性以及数学公式的复杂性，电磁学一直是大学物理课程教学中的难点。为使学生获得直观的理解，需要在相关教学中采用可视化的教学方法，将电、磁场大小与方向的空间分布进行展示。针对这一问题，以“载流螺线管周围的磁场分布”作为教学实例，开展基于Matlab的可视化教学研究。通过采用逐点数值计算的方法，精确和形象地展示磁感应强度大小与方向的空间分布，实现了磁场的可视化演示。在此基础上，通过Matlab GUI构建具有交互界面的教学演示平台。这一方法能够加深学生对于相关概念的理解，提升课堂的教学效果。},
	language = {zh-CN},
	number = {10},
	urldate = {2023-12-05},
	journal = {黑河学院学报},
	author = {张, 建东 and 王, 帅},
	year = {2022},
	keywords = {⛔ No DOI found, 磁感应强度, 电磁学, 可视化演示, Matlab GUI},
	pages = {90--92},
}

@article{_matlab_2022-1,
	title = {{利用MATLAB} {GUI仿真实验辅助中学物理电磁场教学}},
	volume = {40},
	issn = {1003-6148},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=TzO8JwpG6ujVqsD6X3USU49jH5p0gFAlVRiDxLKPgBGUpuRUIVHQmwozT9za4AgMQAlLPbMu9HRMHhoidSU92YfT3ywKGurQ6vs8dz-7Pm9Y7MHi-zqIZsObn6TJzO5q96TjeHqIf5ip4c08gxjvXw==&uniplatform=NZKPT&language=CHS},
	abstract = {高中物理电磁学实验教学面临着部分器材昂贵、操作困难、实验现象不易观测等诸多困难。MATLAB具有不断扩展的源包，可以轻松实现电磁学方程计算和相关模型建构。但是，目前基于MATLAB程序开发的仿真实验在使用过程中通常需要在输入控制平台中进行程序编制，缺少直观的图形用户界面(GUI)，导致仿真实验使用的交互性较差，不利于中学物理仿真实验教学的推广。构建了基于MATLAB GUI的虚拟仿真实验案例，通过设计友好、简洁的图形用户界面，直观呈现带电粒子在电磁场中的运动规律以及电场线和电势线的分布规律,并给出了利用MATLAB GUI仿真实验进行中学物理电磁场教学的方法和技术路线。},
	language = {zh-CN},
	number = {4},
	urldate = {2023-12-05},
	journal = {物理教学探讨},
	author = {谭, 伟 and 杨, 珍珂 and 贾, 伟尧},
	year = {2022},
	keywords = {⛔ No DOI found, 电磁场教学, 图形用户界面, 中学物理, MATLAB},
	pages = {70--72},
}

